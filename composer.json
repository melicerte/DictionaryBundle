{
    "name":         "biig/dictionary-bundle",
    "description":  "Are you often tired to repeat static choices like gender or civility in your apps ?",
    "keywords":     ["dictionary"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name": "Knplabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "BiiG",
            "homepage": "https://www.biig.fr"
        },
        {
            "name": "Shivoham",
            "homepage": "https://github.com/Shivoham"
        },
        {
            "name": "PedroTroller",
            "homepage": "https://github.com/PedroTroller"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0",
        "twig/twig": "^2.0",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "bossa/phpspec2-expect": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "leanphp/phpspec-code-coverage": "^4.0",
        "phpunit/php-code-coverage": "^5.2.3",
        "phpspec/phpspec": "^4.0",
        "phpspec/prophecy": "^1.7",
        "sebastian/comparator": "^2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Knp\\DictionaryBundle\\": "src/Knp/DictionaryBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "suggest": {
        "knplabs/rad-fixtures-load": "Allows to autoregister the dictionary provider as an alice provider"
    }
}
